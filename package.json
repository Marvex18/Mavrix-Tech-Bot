{
  "name": "mavrix-bot",
  "version": "1.0.0",
  "description": "WhatsApp Bot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:optimized": "node --max-old-space-size=512 --optimize-for-size index.js",
    "start:debug": "node --trace-warnings index.js",
    "start:fix": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps && npm start",
    "cleanup": "node cleanup.js",
    "reset-session": "node reset-session.js",
    "start:clean": "npm run cleanup && npm run start:optimized",
    "start:fresh": "npm run reset-session && npm start",
    "install:panel": "npm install --legacy-peer-deps",
    "install:force": "npm install --force",
    "install:clean": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t mavrix-bot .",
    "docker:run": "docker run -e SESSION_ID=$SESSION_ID mavrix-bot"
  },
  "keywords": ["whatsapp-bot"],
  "author": "Mavrix Tech",
  "license": "ISC",
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^7.0.0",
    "awesome-phonenumber": "^5.9.0",
    "axios": "^1.8.4",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "file-type": "^16.5.4",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.1",
    "fs-extra": "^11.2.0",
    "gtts": "^0.2.1",
    "jimp": "^1.6.0",
    "jsdom": "^22.1.0",
    "libphonenumber-js": "^1.11.18",
    "link-preview-js": "^3.0.5",
    "moment-timezone": "^0.5.43",
    "mumaker": "^2.0.0",
    "node-cache": "^5.1.2",
    "node-id3": "^0.2.3",
    "node-webpmux": "^3.1.0",
    "pino": "^8.21.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.32.6",
    "translate-google-api": "^1.0.4",
    "ws": "^8.17.1",
    "yt-search": "^2.12.1",
    "ytdl-core": "^4.11.5"
  },
  "overrides": {
    "jimp": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
